{{!
    @template local/restrict

    This template renders summary information about a single quiz attempt.

    Example context (json):
    {
        context = [
            'header' => header,
            'form' => example_template_context
        ];
    }
}}

<div class="container">
    <div class="local-restrict-content">
        {{!
            @template local_restrict/header
        }}

        {{#header}}
            {{> local_restrict/header }}
        {{/header}}




        <div class="about">
            {{!
                @template local_restrict/forms
            }}
            <div class="msg-div">
                <span class=""></span>
            </div>
            {{#form}}
                {{> local_restrict/forms}}
            {{/form}}
        </div>
    </div>
</div>



